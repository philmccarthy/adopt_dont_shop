<h2>Start an Application</h2>
<p>Please fill in this form to start your application.</p>

<%= form_tag '/applications', method: :post, id: "new-app" do %>

  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :street %>
  <%= text_field_tag :street %><br>
  <%= label_tag :city %>
  <%= text_field_tag :city %><br>
  <%= label_tag :state %>
  <%= text_field_tag :state %><br>
  <%= label_tag :postal_code %>
  <%= text_field_tag :postal_code %><br>

  <%= submit_tag 'Submit' %>
<% end %>