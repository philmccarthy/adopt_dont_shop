<% shelters.each do |shelter| %>
  <section id="shelter-<%=shelter.id%>">
    <h1><%= link_to "#{shelter.name}", shelter_path(shelter) %></h1>

    <%= button_to 'Edit Shelter', edit_shelter_path(shelter), method: :get %>
    
    <%= button_to "Delete Shelter", shelter_path(shelter), method: :delete %></br>
  </section>
<% end %>